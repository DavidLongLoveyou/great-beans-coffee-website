<svg width="800" height="600" viewBox="0 0 800 600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E0F6FF;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="groundGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#D2B48C;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8B7355;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="buildingGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#F5F5DC;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#D2B48C;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Sky background -->
  <rect width="800" height="300" fill="url(#skyGradient)"/>
  
  <!-- Ground -->
  <rect y="300" width="800" height="300" fill="url(#groundGradient)"/>
  
  <!-- Processing facility building -->
  <rect x="200" y="200" width="400" height="200" fill="url(#buildingGradient)" stroke="#8B7355" stroke-width="2"/>
  
  <!-- Roof -->
  <polygon points="180,200 400,150 620,200" fill="#8B4513"/>
  
  <!-- Chimney with smoke -->
  <rect x="480" y="120" width="20" height="80" fill="#696969"/>
  <rect x="475" y="115" width="30" height="10" fill="#696969"/>
  
  <!-- Smoke -->
  <g transform="translate(490,115)">
    <circle cx="0" cy="-10" r="8" fill="#D3D3D3" opacity="0.7"/>
    <circle cx="-5" cy="-25" r="10" fill="#D3D3D3" opacity="0.6"/>
    <circle cx="8" cy="-35" r="12" fill="#D3D3D3" opacity="0.5"/>
    <circle cx="-3" cy="-50" r="9" fill="#D3D3D3" opacity="0.4"/>
    <circle cx="10" cy="-60" r="11" fill="#D3D3D3" opacity="0.3"/>
  </g>
  
  <!-- Windows -->
  <rect x="230" y="230" width="40" height="30" fill="#87CEEB" stroke="#654321" stroke-width="2"/>
  <rect x="250" y="230" width="2" height="30" fill="#654321"/>
  <rect x="230" y="245" width="40" height="2" fill="#654321"/>
  
  <rect x="320" y="230" width="40" height="30" fill="#87CEEB" stroke="#654321" stroke-width="2"/>
  <rect x="340" y="230" width="2" height="30" fill="#654321"/>
  <rect x="320" y="245" width="40" height="2" fill="#654321"/>
  
  <rect x="530" y="230" width="40" height="30" fill="#87CEEB" stroke="#654321" stroke-width="2"/>
  <rect x="550" y="230" width="2" height="30" fill="#654321"/>
  <rect x="530" y="245" width="40" height="2" fill="#654321"/>
  
  <!-- Main entrance door -->
  <rect x="380" y="320" width="40" height="80" fill="#654321"/>
  <circle cx="415" cy="360" r="3" fill="#FFD700"/>
  
  <!-- Loading dock -->
  <rect x="150" y="350" width="80" height="50" fill="#8B7355"/>
  <rect x="140" y="340" width="100" height="10" fill="#696969"/>
  
  <!-- Coffee processing equipment inside (visible through windows) -->
  <g opacity="0.6">
    <!-- Pulping machine -->
    <rect x="240" y="280" width="20" height="15" fill="#C0C0C0"/>
    <circle cx="250" cy="287" r="5" fill="#696969"/>
    
    <!-- Drying beds -->
    <rect x="330" y="280" width="25" height="15" fill="#8B4513"/>
    <circle cx="335" cy="285" r="2" fill="#654321"/>
    <circle cx="340" cy="287" r="2" fill="#654321"/>
    <circle cx="345" cy="285" r="2" fill="#654321"/>
    <circle cx="350" cy="287" r="2" fill="#654321"/>
  </g>
  
  <!-- Workers outside -->
  <!-- Worker 1 carrying sacks -->
  <g transform="translate(120,380)">
    <circle cx="0" cy="-40" r="12" fill="#DEB887"/>
    <rect x="-8" y="-28" width="16" height="25" fill="#4169E1"/>
    <rect x="-6" y="-3" width="12" height="20" fill="#8B4513"/>
    <rect x="-3" y="17" width="6" height="15" fill="#DEB887"/>
    <rect x="3" y="17" width="6" height="15" fill="#DEB887"/>
    <rect x="-3" y="32" width="6" height="8" fill="#654321"/>
    <rect x="3" y="32" width="6" height="8" fill="#654321"/>
    <!-- Hat -->
    <ellipse cx="0" cy="-45" rx="15" ry="5" fill="#8B4513"/>
    <!-- Sack on shoulder -->
    <rect x="15" y="-25" width="12" height="20" fill="#D2691E"/>
    <rect x="17" y="-23" width="8" height="16" fill="#8B4513"/>
  </g>
  
  <!-- Worker 2 operating equipment -->
  <g transform="translate(280,380)">
    <circle cx="0" cy="-40" r="12" fill="#DEB887"/>
    <rect x="-8" y="-28" width="16" height="25" fill="#DC143C"/>
    <rect x="-6" y="-3" width="12" height="20" fill="#000080"/>
    <rect x="-3" y="17" width="6" height="15" fill="#DEB887"/>
    <rect x="3" y="17" width="6" height="15" fill="#DEB887"/>
    <rect x="-3" y="32" width="6" height="8" fill="#654321"/>
    <rect x="3" y="32" width="6" height="8" fill="#654321"/>
    <!-- Hat -->
    <ellipse cx="0" cy="-45" rx="15" ry="5" fill="#8B4513"/>
  </g>
  
  <!-- Coffee drying area -->
  <g transform="translate(650,350)">
    <!-- Drying platform -->
    <rect x="-50" y="0" width="100" height="50" fill="#D2B48C" stroke="#8B7355" stroke-width="2"/>
    <!-- Coffee beans drying -->
    <circle cx="-40" cy="10" r="2" fill="#654321"/>
    <circle cx="-30" cy="15" r="2" fill="#654321"/>
    <circle cx="-20" cy="12" r="2" fill="#654321"/>
    <circle cx="-10" cy="18" r="2" fill="#654321"/>
    <circle cx="0" cy="14" r="2" fill="#654321"/>
    <circle cx="10" cy="20" r="2" fill="#654321"/>
    <circle cx="20" cy="16" r="2" fill="#654321"/>
    <circle cx="30" cy="22" r="2" fill="#654321"/>
    <circle cx="40" cy="18" r="2" fill="#654321"/>
    <circle cx="-35" cy="25" r="2" fill="#654321"/>
    <circle cx="-25" cy="30" r="2" fill="#654321"/>
    <circle cx="-15" cy="27" r="2" fill="#654321"/>
    <circle cx="-5" cy="33" r="2" fill="#654321"/>
    <circle cx="5" cy="29" r="2" fill="#654321"/>
    <circle cx="15" cy="35" r="2" fill="#654321"/>
    <circle cx="25" cy="31" r="2" fill="#654321"/>
    <circle cx="35" cy="37" r="2" fill="#654321"/>
    
    <!-- Worker tending to drying beans -->
    <g transform="translate(0,-50)">
      <circle cx="0" cy="-40" r="10" fill="#DEB887"/>
      <rect x="-6" y="-30" width="12" height="20" fill="#FF69B4"/>
      <rect x="-5" y="-10" width="10" height="15" fill="#8B4513"/>
      <rect x="-2" y="5" width="4" height="12" fill="#DEB887"/>
      <rect x="2" y="5" width="4" height="12" fill="#DEB887"/>
      <rect x="-2" y="17" width="4" height="6" fill="#654321"/>
      <rect x="2" y="17" width="4" height="6" fill="#654321"/>
      <!-- Hat -->
      <ellipse cx="0" cy="-45" rx="12" ry="4" fill="#8B4513"/>
      <!-- Rake -->
      <rect x="-20" y="-15" width="3" height="25" fill="#8B4513"/>
      <rect x="-25" y="-18" width="10" height="3" fill="#8B4513"/>
    </g>
  </g>
  
  <!-- Coffee sacks -->
  <g transform="translate(100,450)">
    <rect x="0" y="0" width="25" height="35" fill="#D2691E"/>
    <rect x="2" y="2" width="21" height="31" fill="#8B4513"/>
    <text x="12" y="20" text-anchor="middle" font-family="Arial" font-size="8" fill="#FFFFFF">COFFEE</text>
  </g>
  
  <g transform="translate(130,450)">
    <rect x="0" y="0" width="25" height="35" fill="#D2691E"/>
    <rect x="2" y="2" width="21" height="31" fill="#8B4513"/>
    <text x="12" y="20" text-anchor="middle" font-family="Arial" font-size="8" fill="#FFFFFF">COFFEE</text>
  </g>
  
  <g transform="translate(160,450)">
    <rect x="0" y="0" width="25" height="35" fill="#D2691E"/>
    <rect x="2" y="2" width="21" height="31" fill="#8B4513"/>
    <text x="12" y="20" text-anchor="middle" font-family="Arial" font-size="8" fill="#FFFFFF">COFFEE</text>
  </g>
  
  <!-- Truck for transportation -->
  <g transform="translate(50,420)">
    <!-- Truck body -->
    <rect x="0" y="0" width="80" height="30" fill="#4169E1"/>
    <rect x="80" y="5" width="30" height="25" fill="#4169E1"/>
    <!-- Wheels -->
    <circle cx="15" cy="35" r="8" fill="#2F4F4F"/>
    <circle cx="15" cy="35" r="5" fill="#C0C0C0"/>
    <circle cx="65" cy="35" r="8" fill="#2F4F4F"/>
    <circle cx="65" cy="35" r="5" fill="#C0C0C0"/>
    <!-- Windshield -->
    <rect x="82" y="8" width="26" height="15" fill="#87CEEB"/>
    <!-- Headlight -->
    <circle cx="112" cy="15" r="3" fill="#FFFF00"/>
  </g>
  
  <!-- Sun -->
  <circle cx="150" cy="80" r="40" fill="#FFD700" opacity="0.9"/>
  <g transform="translate(150,80)">
    <rect x="-2" y="-60" width="4" height="20" fill="#FFD700"/>
    <rect x="-2" y="40" width="4" height="20" fill="#FFD700"/>
    <rect x="-60" y="-2" width="20" height="4" fill="#FFD700"/>
    <rect x="40" y="-2" width="20" height="4" fill="#FFD700"/>
    <rect x="-45" y="-45" width="4" height="15" fill="#FFD700" transform="rotate(45)"/>
    <rect x="-45" y="30" width="4" height="15" fill="#FFD700" transform="rotate(-45)"/>
    <rect x="30" y="-45" width="4" height="15" fill="#FFD700" transform="rotate(-45)"/>
    <rect x="30" y="30" width="4" height="15" fill="#FFD700" transform="rotate(45)"/>
  </g>
  
  <!-- Title -->
  <text x="400" y="580" text-anchor="middle" font-family="Arial, sans-serif" font-size="32" font-weight="bold" fill="#2F4F4F">Coffee Processing</text>
</svg>