<svg width="800" height="600" viewBox="0 0 800 600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E0F6FF;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="groundGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#90EE90;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#228B22;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Sky background -->
  <rect width="800" height="400" fill="url(#skyGradient)"/>
  
  <!-- Ground -->
  <rect y="400" width="800" height="200" fill="url(#groundGradient)"/>
  
  <!-- Coffee plantation hills -->
  <path d="M0,350 Q200,300 400,320 Q600,340 800,310 L800,400 L0,400 Z" fill="#8FBC8F"/>
  
  <!-- Coffee plants -->
  <g transform="translate(100,380)">
    <rect x="-2" y="0" width="4" height="20" fill="#8B4513"/>
    <ellipse cx="0" cy="-5" rx="15" ry="10" fill="#228B22"/>
    <circle cx="-8" cy="-3" r="3" fill="#DC143C"/>
    <circle cx="8" cy="-7" r="3" fill="#DC143C"/>
  </g>
  
  <g transform="translate(200,390)">
    <rect x="-2" y="0" width="4" height="20" fill="#8B4513"/>
    <ellipse cx="0" cy="-5" rx="15" ry="10" fill="#228B22"/>
    <circle cx="-6" cy="-5" r="3" fill="#DC143C"/>
    <circle cx="6" cy="-3" r="3" fill="#DC143C"/>
  </g>
  
  <g transform="translate(300,385)">
    <rect x="-2" y="0" width="4" height="20" fill="#8B4513"/>
    <ellipse cx="0" cy="-5" rx="15" ry="10" fill="#228B22"/>
    <circle cx="-7" cy="-6" r="3" fill="#DC143C"/>
    <circle cx="9" cy="-4" r="3" fill="#DC143C"/>
  </g>
  
  <!-- Farmers working together -->
  <!-- Farmer 1 -->
  <g transform="translate(150,350)">
    <circle cx="0" cy="-40" r="12" fill="#DEB887"/>
    <rect x="-8" y="-28" width="16" height="25" fill="#4169E1"/>
    <rect x="-6" y="-3" width="12" height="20" fill="#8B4513"/>
    <rect x="-3" y="17" width="6" height="15" fill="#DEB887"/>
    <rect x="3" y="17" width="6" height="15" fill="#DEB887"/>
    <rect x="-3" y="32" width="6" height="8" fill="#654321"/>
    <rect x="3" y="32" width="6" height="8" fill="#654321"/>
    <!-- Hat -->
    <ellipse cx="0" cy="-45" rx="15" ry="5" fill="#8B4513"/>
    <!-- Basket -->
    <ellipse cx="20" cy="-10" rx="8" ry="12" fill="#D2691E" stroke="#8B4513" stroke-width="2"/>
  </g>
  
  <!-- Farmer 2 -->
  <g transform="translate(250,350)">
    <circle cx="0" cy="-40" r="12" fill="#DEB887"/>
    <rect x="-8" y="-28" width="16" height="25" fill="#DC143C"/>
    <rect x="-6" y="-3" width="12" height="20" fill="#000080"/>
    <rect x="-3" y="17" width="6" height="15" fill="#DEB887"/>
    <rect x="3" y="17" width="6" height="15" fill="#DEB887"/>
    <rect x="-3" y="32" width="6" height="8" fill="#654321"/>
    <rect x="3" y="32" width="6" height="8" fill="#654321"/>
    <!-- Hat -->
    <ellipse cx="0" cy="-45" rx="15" ry="5" fill="#8B4513"/>
    <!-- Tool -->
    <rect x="-25" y="-15" width="3" height="30" fill="#8B4513"/>
    <rect x="-30" y="-18" width="13" height="6" fill="#C0C0C0"/>
  </g>
  
  <!-- Community building -->
  <g transform="translate(500,320)">
    <rect x="0" y="0" width="80" height="60" fill="#DEB887"/>
    <polygon points="0,0 40,-20 80,0" fill="#8B4513"/>
    <rect x="15" y="20" width="15" height="25" fill="#654321"/>
    <rect x="50" y="15" width="20" height="20" fill="#87CEEB"/>
    <rect x="55" y="20" width="2" height="15" fill="#654321"/>
    <rect x="63" y="20" width="2" height="15" fill="#654321"/>
    <rect x="55" y="25" width="10" height="2" fill="#654321"/>
  </g>
  
  <!-- People gathering -->
  <!-- Person 1 -->
  <g transform="translate(450,380)">
    <circle cx="0" cy="-15" r="8" fill="#DEB887"/>
    <rect x="-6" y="-7" width="12" height="15" fill="#228B22"/>
    <rect x="-4" y="8" width="8" height="12" fill="#000080"/>
  </g>
  
  <!-- Person 2 -->
  <g transform="translate(470,380)">
    <circle cx="0" cy="-15" r="8" fill="#DEB887"/>
    <rect x="-6" y="-7" width="12" height="15" fill="#FF69B4"/>
    <rect x="-4" y="8" width="8" height="12" fill="#8B4513"/>
  </g>
  
  <!-- Person 3 -->
  <g transform="translate(530,380)">
    <circle cx="0" cy="-15" r="8" fill="#DEB887"/>
    <rect x="-6" y="-7" width="12" height="15" fill="#FFD700"/>
    <rect x="-4" y="8" width="8" height="12" fill="#000080"/>
  </g>
  
  <!-- Person 4 -->
  <g transform="translate(550,380)">
    <circle cx="0" cy="-15" r="8" fill="#DEB887"/>
    <rect x="-6" y="-7" width="12" height="15" fill="#FF4500"/>
    <rect x="-4" y="8" width="8" height="12" fill="#8B4513"/>
  </g>
  
  <!-- Sun -->
  <circle cx="700" cy="80" r="40" fill="#FFD700" opacity="0.9"/>
  <g transform="translate(700,80)">
    <rect x="-2" y="-60" width="4" height="20" fill="#FFD700"/>
    <rect x="-2" y="40" width="4" height="20" fill="#FFD700"/>
    <rect x="-60" y="-2" width="20" height="4" fill="#FFD700"/>
    <rect x="40" y="-2" width="20" height="4" fill="#FFD700"/>
    <rect x="-45" y="-45" width="4" height="15" fill="#FFD700" transform="rotate(45)"/>
    <rect x="-45" y="30" width="4" height="15" fill="#FFD700" transform="rotate(-45)"/>
    <rect x="30" y="-45" width="4" height="15" fill="#FFD700" transform="rotate(-45)"/>
    <rect x="30" y="30" width="4" height="15" fill="#FFD700" transform="rotate(45)"/>
  </g>
  
  <!-- Hearts representing community love -->
  <g transform="translate(400,200)">
    <path d="M0,5 C0,0 -5,-5 -10,0 C-15,-5 -20,0 -20,5 C-20,15 0,25 0,25 C0,25 20,15 20,5 C20,0 15,-5 10,0 C5,-5 0,0 0,5" fill="#FF69B4" opacity="0.7"/>
  </g>
  
  <g transform="translate(350,150)">
    <path d="M0,3 C0,0 -3,-3 -6,0 C-9,-3 -12,0 -12,3 C-12,9 0,15 0,15 C0,15 12,9 12,3 C12,0 9,-3 6,0 C3,-3 0,0 0,3" fill="#FF1493" opacity="0.6"/>
  </g>
  
  <!-- Title -->
  <text x="400" y="550" text-anchor="middle" font-family="Arial, sans-serif" font-size="32" font-weight="bold" fill="#2F4F4F">Community Partnership</text>
</svg>